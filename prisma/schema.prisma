generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model articles {
  urlid             String  @id
  headline          String?
  contents          String?
  authors           String?
  uploaddate        String?
  readtime          Int?
  imageurl          String?
  imagedescription  String?
  scrapingtimestamp String?
}

model tweets {
  id               BigInt    @id @unique(map: "idx_tweet_id")
  id_str           String?
  tweet_url        String?
  publish_date     DateTime? @db.Timestamp(6)
  tweet_user       String?
  lang             String?
  rawcontent       String?
  replycount       Decimal?  @db.Decimal
  retweetcount     Decimal?  @db.Decimal
  likecount        Decimal?  @db.Decimal
  quotecount       Decimal?  @db.Decimal
  conversationid   String?
  hashtags         String?
  cashtags         String?
  mentionedusers   String?
  links            String?
  viewcount        Decimal?  @db.Decimal
  retweetedtweet   String?
  quotedtweet      String?
  place            String?
  coordinates      String?
  inreplytotweetid String?
  inreplytouser    String?
  source           String?
  sourceurl        String?
  sourcelabel      String?
  media            String?
  type             String?   @map("_type")
}
